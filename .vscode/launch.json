{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Python: Debug Pytest",
        "type": "python",
        "request": "launch",
        "module": "pytest",
        "justMyCode": false,
        "args": [
          "--maxfail=1",
          "--disable-warnings",
          "-q"
        ],
        "env": { "PYTHONPATH": "${workspaceFolder}" }
      }
    ]
  }
  